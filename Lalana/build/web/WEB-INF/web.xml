<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>InsertPatient</servlet-name>
        <servlet-class>controlleur.patient.InsertPatient</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChoixEtat</servlet-name>
        <servlet-class>controlleur.TraitementDent.ChoixEtat</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertEtatDent</servlet-name>
        <servlet-class>controlleur.TraitementDent.InsertEtatDent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Proposition</servlet-name>
        <servlet-class>controlleur.TraitementDent.Proposition</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VoirDetail</servlet-name>
        <servlet-class>controlleur.TraitementDent.VoirDetail</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertPatient</servlet-name>
        <url-pattern>/InsertPatient</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChoixEtat</servlet-name>
        <url-pattern>/ChoixEtat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertEtatDent</servlet-name>
        <url-pattern>/InsertEtatDent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Proposition</servlet-name>
        <url-pattern>/Proposition</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VoirDetail</servlet-name>
        <url-pattern>/VoirDetail</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
